{
  "name": "stock-discovery-agent",
  "version": "1.0.0",
  "description": "Production-ready agent for NSE stock data with CSV integration",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:prod": "NODE_ENV=production node src/index.js",
    "dev": "NODE_ENV=development nodemon src/index.js",
    "test": "node tests/ProductionNSEService.test.js",
    "test:production": "node final-production-test.js",
    "test:coverage": "nyc node tests/ProductionNSEService.test.js",
    "health": "node scripts/health-check.js",
    "cache:clear": "node scripts/clear-cache.js",
    "validate:config": "node scripts/validate-config.js",
    "docker:build": "docker build -t stock-agent .",
    "docker:run": "docker run -p 3001:3001 stock-agent",
    "lint": "eslint src/ --fix",
    "format": "prettier --write src/"
  },
  "keywords": [
    "stocks",
    "nse",
    "csv",
    "production",
    "agent",
    "finance",
    "api"
  ],
  "author": "Stock Sentiment Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.4",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "mongodb": "^6.18.0",
    "mongoose": "^8.17.1",
    "node-cron": "^3.0.3",
    "puppeteer": "^24.16.2",
    "redis": "^4.7.1",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "eslint": "^9.9.0",
    "nodemon": "^3.1.4",
    "nyc": "^17.0.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/stock-sentiment.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/stock-sentiment/issues"
  },
  "homepage": "https://github.com/your-org/stock-sentiment#readme"
}
